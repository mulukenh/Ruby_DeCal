<h2>Cats Information</h2>
<table>                         
  <tr>                          
    <th>name</th>
  </tr>
<% @cats.each do |cat| %>      
  <tr>
    <td><%= cat.name %></td>  
  </tr>
<% end %>                        
</table>  

<h2>Todos Information</h2>
<table>                         
  <tr>                          
    <th>task</th>
    <th>finished</th>
  </tr>
<% @todos.each do |todo| %>      
  <tr>
    <td><%= todo.task %></td>  
    <td><%= todo.finished %></td> 
  </tr>
<% end %>                        
</table> 

<h2>Users Information</h2>
<table>                         
  <tr>                          
    <th>username</th>
    <th>email</th>
    <th>age</th>
  </tr>
<% @users.each do |user| %>      
  <tr>
    <td><%= user.username %></td>  
    <td><%= user.email %></td> 
    <td><%= user.age %></td> 
  </tr>
<% end %>      
</table> 

<%= link_to "Back to the Home page", '/'%>